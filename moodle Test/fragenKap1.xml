<?xml version="1.0" encoding="UTF-8"?>
<quiz></span>
    <!-- Frage 1: Wahr oder Falsch -->
    <question type="truefalse">
        <name>
            <text>Basiswissen Processing</text>
        </name>
        <questiontext format="html">
            <text><![CDATA[Processing ist eine Programmiersprache, die speziell für die visuelle Datenverarbeitung entwickelt wurde.]]></text>
        </questiontext>
        <defaultgrade>1.0000000</defaultgrade>
        <penalty>0.3333333</penalty>
        <hidden>0</hidden>
        <idnumber></idnumber>
        <answer fraction="100" format="html">
            <text><![CDATA[true]]></text>
            <feedback format="html">
                <text><![CDATA[Korrekt! Processing ist tatsächlich eine Programmiersprache für visuelle Datenverarbeitung.]]></text>
            </feedback>
        </answer>
    </question>

    <!-- Frage 2: Kurze Antwort -->
    <question type="shortanswer">
        <name>
            <text>Grundfunktion in Processing</text>
        </name>
        <questiontext format="html">
            <text><![CDATA[Wie heißt die Funktion in Processing, die für die initiale Einstellung des Zeichnungsbereichs zuständig ist?]]></text>
        </questiontext>
        <defaultgrade>1.0000000</defaultgrade>
        <penalty>0.3333333</penalty>
        <hidden>0</hidden>
        <idnumber></idnumber>
        <answer fraction="100" format="html">
            <text><![CDATA[setup]]></text>
            <feedback format="html">
                <text><![CDATA[Richtig! Die setup()-Funktion wird einmal zu Beginn eines Processing-Programms ausgeführt.]]></text>
            </feedback>
        </answer>
    </question>
</quiz>
